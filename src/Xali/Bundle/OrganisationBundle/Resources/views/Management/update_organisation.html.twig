{# src/OrganisationBundle/Resources/views/Management/update_organisation.html.twig #}

{% extends "XaliOrganisationBundle::base_organisation.html.twig" %}

{#Overwrite organisation_body block#}
{% block organisation_body %}
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">    
    <form id="" method="" action="">
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="name">{{ "form.name"|trans }}</label>
                <input type="text" class="form-control" id="name" />
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="creating_date">{{ "form.creating_date"|trans }}</label>
                <input type="text" class="form-control" id="creatin_date" />
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="email">{{ "form.email"|trans }}</label>
                <input type="text" type="email" class="form-control" id="email" />
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="confirm_email">{{ "form.confirm_email"|trans }}</label>
                <input type="email" class="form-control" id="confirm_email" />
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="password">{{ "form.password"|trans }}</label>
                <input type="password" class="form-control" id="password" />
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="form-group">
                <label for="confirm_password">{{ "form.confirm_password"|trans }}</label>
                <input type="password" class="form-control" id="confirm_password" />
            </div>
        </div>
        <input type="submit" class="form-control btn btn-primary" value="{{ "form.submit"|trans }}" />
    </form>
</div>
        
        
        
        
        
        
{% endblock %}